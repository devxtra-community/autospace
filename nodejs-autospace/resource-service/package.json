{
  "name": "resource-service",
  "private": true,
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch -r dotenv/config src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "lint": "pnpm eslint src/**/*.ts",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "typecheck": "tsc --noEmit",
    "typeorm": "typeorm-ts-node-commonjs -d src/db/data-source.ts",
    "migration:generate": "pnpm typeorm migration:generate src/db/migrations/CreateValetsTable",
    "migration:run": "pnpm typeorm migration:run",
    "migration:revert": "pnpm typeorm migration:revert"
  },
  "dependencies": {
    "@autospace/shared": "workspace:*",
    "@aws-sdk/client-s3": "^3.978.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "form-data": "^4.0.5",
    "jsonwebtoken": "^9.0.3",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.28",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.8",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typeorm-ts-node-commonjs": "^0.3.20"
  }
}
